[rpc]
ip = 127.0.0.1
port = 8333
user = yourUser
password = yourPwd

[console]
# rpc_update_freq in seconds(for info live view)
rpc_update_freq = 60    
font_color = white
default_view = info

[stats]
# hashrate_blocks is hashrate from previous n blocks
# fee_estimate_blocks is target blocks n of estimatesmartfee rpc
hashrate_blocks = 100
fee_estimate_blocks = 6

